<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    
    <class name="com.society.hibernate.ProductBillingHibernate" 
    table="product_billing_details" schema="society"
    >
    
    	<id name="getPkProductBillingId" type="java.lang.Long" column="pk_product_Billing_id">
			<generator class="increment" />
		</id>


    <property
        name="fkVendorId"
        type="java.lang.Long"
        column="fk_vendor_id"
    />

	<property
        name="vendorName"
        type="java.lang.String"
        column="vendor_name"
    />
    
    
    <property
        name="billingDate"
        type="java.sql.Date"
        column="date_of_billing"
    />
    
<!--     <property
        name="srNO"
        type="java.lang.Long"
        column="sr_number"
    />	 -->
    
    <property
        name="description1"
        type="java.lang.String"
        column="description"
    />
    
    <property
        name="quantity1"
        type="java.lang.Long"
        column="quantity"
    />
    
    <property
        name="unit1"
        type="java.lang.Double"
        column="unit_price"
    />
    
    <property
        name="amount1"
        type="java.lang.Double"
        column="amount"
    />
    
     <property
        name="subTotal1"
        type="java.lang.Double"
        column="sub_total"
    />
    
    <property
        name="gst1"
        type="java.lang.Double"
        column="gst"
    />
    
     <property
        name="vat1"
        type="java.lang.Double"
        column="vat"
    />
    
    <property
        name="grossTotal2"
        type="java.lang.Double"
        column="gross_total"
    />
    
    
        <property
        name="billNo"
        type="java.lang.String"
        column="bill_nos"
    />
    
    </class>
</hibernate-mapping>
   